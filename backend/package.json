{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "setup": "prisma migrate reset && mkdir uploads",
    "reset": "prisma migrate reset && cd uploads && rm -rf .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.ts",
    "build": "rm -rf ./dist && tsc",
    "start": "node dist/index.js",
    "precompile": "rm -rf ./compile && mkdir compile && npm run build",
    "compile": "npm run precompile && esbuild dist/index.js  --bundle --outfile=compile/build.cjs --format=cjs --platform=node && cd compile && npx pkg build.cjs && npm run postcompile",
    "postcompile": "cd compile && rm -rf build.cjs"
  },
  "keywords": [],
  "author": "Th3BossC",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "@types/node-os-utils": "^1.3.4",
    "esbuild": "^0.24.0",
    "express": "^4.21.0",
    "nexe": "^4.0.0-rc.6",
    "nodemon": "^3.1.7",
    "pkg": "^5.8.1",
    "prisma": "^5.20.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "express-async-handler": "^1.2.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "node-os-utils": "^1.3.7",
    "socket.io": "^4.8.0",
    "ts-node": "^10.9.2"
  }
}