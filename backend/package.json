{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "generate-large-file": "base64 /dev/urandom | head -c 4G > public/4gb.txt",
    "docker-build": "docker build -t expserver-tester-image .",
    "setup": "prisma migrate dev --name dev && mkdir uploads && npm run docker-build",
    "reset": "prisma migrate reset && cd uploads && rm -rf *",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_OPTIONS=\"--max-old-space-size=4096\" nodemon src/index.ts",
    "build": "rm -rf ./dist && tsc",
    "start": "node --max-old-space-size=4096 dist/index.js",
    "precompile": "rm -rf ./compile && mkdir compile && npm run build",
    "compile": "npm run precompile && esbuild dist/index.js --bundle --outfile=compile/build.cjs --format=cjs --platform=node --external:*.node && cd compile && npx pkg build.cjs && npm run postcompile",
    "postcompile": "cd compile && rm -rf build.cjs"
  },
  "keywords": [],
  "author": "Th3BossC",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/dockerode": "^3.3.32",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "@types/node-os-utils": "^1.3.4",
    "chalk": "^5.3.0",
    "esbuild": "^0.24.0",
    "express": "^4.21.0",
    "nexe": "^4.0.0-rc.6",
    "nodemon": "^3.1.7",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "pkg": "^5.8.1",
    "prisma": "^5.20.0",
    "typescript": "^5.6.2",
    "winston": "^3.17.0"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "eventemitter3": "^5.0.1",
    "express-async-handler": "^1.2.0",
    "http-parser-js": "^0.5.9",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "node-os-utils": "^1.3.7",
    "socket.io": "^4.8.0",
    "ts-node": "^10.9.2"
  }
}